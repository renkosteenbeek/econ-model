  <RULES LABEL="Rules" DESCRIPTION="">
    <RULES LABEL="Wizard Rules" DESCRIPTION="" ISSYSTEM="true">
      <RULES LABEL="DecisionMatrixWizard" DESCRIPTION="" ISSYSTEM="true">
        <RULE LABEL="Armrests@Visible-DMW" ISSYSTEM="true">
          <CONDITION>true</CONDITION>
          <IMPLICATION>Armrests@Visible = ChairModel in {""; "Luxery"; "Ambassador"; "Executive"};
</IMPLICATION>
          <DESCRIPTION>Set the visible of Armrests - Use the value of ChairModel</DESCRIPTION>
          <LHS>;armrests@visible;</LHS>
        </RULE>
        <RULE LABEL="Accessories@Visible-DMW" ISSYSTEM="true">
          <CONDITION>true</CONDITION>
          <IMPLICATION>Accessories@Visible = ChairModel in {""; "Luxery"; "Ambassador"; "Executive"};
</IMPLICATION>
          <DESCRIPTION>Set the visible of Accessories - Use the value of ChairModel</DESCRIPTION>
          <LHS>;accessories@visible;</LHS>
        </RULE>
        <RULE LABEL="Accessories.ExtraStrongQualityOfFabric@Visible-DMW" ISSYSTEM="true">
          <CONDITION>true</CONDITION>
          <IMPLICATION>Accessories.ExtraStrongQualityOfFabric@Visible = Fabric in {""; "Polyester"; "Cotton"};
</IMPLICATION>
          <DESCRIPTION>Set the visible of Accessories.ExtraStrongQualityOfFabric - Use the value of Fabric</DESCRIPTION>
          <LHS>;accessories.extrastrongqualityoffabric@visible;</LHS>
        </RULE>
        <RULE LABEL="ColorBack@Default-DMW" ISSYSTEM="true">
          <CONDITION>true</CONDITION>
          <IMPLICATION>if ColorSeat in {"Blue"} then ColorBack@default = "Blue" end if;
if ColorSeat in {"Red"} then ColorBack@default = "Red" end if;
if ColorSeat in {"Yellow"} then ColorBack@default = "Yellow" end if;
</IMPLICATION>
          <DESCRIPTION>Set the default value of ColorBack - Use the value of ColorSeat</DESCRIPTION>
          <LHS>;colorback@default;</LHS>
        </RULE>
        <RULE LABEL="Armrests@Default-DMW" ISSYSTEM="true">
          <CONDITION>true</CONDITION>
          <IMPLICATION>if ChairModel in {"Ambassador"; "Executive"} then Armrests@default = true end if;
</IMPLICATION>
          <DESCRIPTION>Set the default value of Armrests - Use the value of ChairModel</DESCRIPTION>
          <LHS>;armrests@default;</LHS>
        </RULE>
        <RULE LABEL="Fabric@Options-DMW" ISSYSTEM="true">
          <CONDITION>true</CONDITION>
          <IMPLICATION>Fabric@option("Polyester")@visible = ChairModel in {""; "Standard"; "Luxery"; "Ambassador"; "Executive"};
Fabric@option("Cotton")@visible = ChairModel in {""; "Standard"; "Luxery"; "Ambassador"; "Executive"};
Fabric@option("Velvet")@visible = ChairModel in {""; "Standard"; "Luxery"; "Ambassador"; "Executive"};
Fabric@option("Leather")@visible = ChairModel in {"Executive"};
</IMPLICATION>
          <DESCRIPTION>Set the option visibility of Fabric - Use the value of ChairModel</DESCRIPTION>
          <LHS>;fabric@options;</LHS>
        </RULE>
        <RULE LABEL="ColorArmrests@Visible-DMW" ISSYSTEM="true">
          <CONDITION>true</CONDITION>
          <IMPLICATION>ColorArmrests@Visible = Armrests;
</IMPLICATION>
          <DESCRIPTION>Set the visible of ColorArmrests - Use the value of Armrests</DESCRIPTION>
          <LHS>;colorarmrests@visible;</LHS>
        </RULE>
        <RULE LABEL="ColorBack@Options-DMW" ISSYSTEM="true">
          <CONDITION>true</CONDITION>
          <IMPLICATION>ColorBack@option("Blue")@visible = ColorSeat in {""; "Blue"; "Red"; "Yellow"};
ColorBack@option("Red")@visible = ColorSeat in {""; "Blue"; "Red"};
ColorBack@option("Yellow")@visible = ColorSeat in {""; "Blue"; "Yellow"};
</IMPLICATION>
          <DESCRIPTION>Set the option visibility of ColorBack - Use the value of ColorSeat</DESCRIPTION>
          <LHS>;colorback@options;</LHS>
        </RULE>
        <RULE LABEL="ColorArmrests@Options-DMW" ISSYSTEM="true">
          <CONDITION>true</CONDITION>
          <IMPLICATION>ColorArmrests@option("Blue")@visible = ColorBack in {""; "Blue"; "Red"; "Yellow"};
ColorArmrests@option("Red")@visible = ColorBack in {""; "Blue"; "Red"};
ColorArmrests@option("Yellow")@visible = ColorBack in {""; "Blue"; "Yellow"};
</IMPLICATION>
          <DESCRIPTION>Set the option visibility of ColorArmrests - Use the value of ColorBack</DESCRIPTION>
          <LHS>;colorarmrests@options;</LHS>
        </RULE>
      </RULES>
    </RULES>
    <RULE LABEL="Adjustable armrests only visible with armrests required">
      <CONDITION>true	</CONDITION>
      <IMPLICATION>Accessories.AdjustableArmrests@visible = Armrests;</IMPLICATION>
      <DESCRIPTION>Adjustable armrests only visible with armrests required</DESCRIPTION>
      <LHS>;accessories.adjustablearmrests@visible;</LHS>
    </RULE>
    <RULE LABEL="Rolling wheels and leg rests are not allowed together" TYPE="validation">
      <CONDITION>Accessories.RollingWheels == true and Accessories.LegRests == true	</CONDITION>
      <IMPLICATION>"Rolling wheels and leg rests are not allowed together!"</IMPLICATION>
      <DESCRIPTION>Rolling wheels and leg rests are not allowed together</DESCRIPTION>
      <LHS>;</LHS>
    </RULE>
    <RULE LABEL="Height standard chair is 900 mm">
      <CONDITION>ChairModel == "Standard"	</CONDITION>
      <IMPLICATION>Height = "900";</IMPLICATION>
      <DESCRIPTION>Height standard chair is 900 mm</DESCRIPTION>
      <LHS>;height@value;</LHS>
    </RULE>
    <RULE LABEL="Only luxery chair can be 1000 mm height">
      <CONDITION>true	</CONDITION>
      <IMPLICATION>Height@option("1000")@visible = ChairModel == "Luxery";</IMPLICATION>
      <DESCRIPTION>Only luxery chair can be 1000 mm height</DESCRIPTION>
      <LHS>;height@options;</LHS>
    </RULE>
    <RULE LABEL="Define item name">
      <CONDITION>true</CONDITION>
      <IMPLICATION>DESCRIPTION = "Chair " &amp; ChairModel;

</IMPLICATION>
      <DESCRIPTION>Define item name</DESCRIPTION>
      <LHS>;description@value;</LHS>
    </RULE>
    <RULE LABEL="Rule1">
      <CONDITION>true</CONDITION>
      <IMPLICATION>if ChairModel == "Standard" then
	Armrests = false
end if;</IMPLICATION>
      <DESCRIPTION>Rule1</DESCRIPTION>
      <LHS>;armrests@value;</LHS>
    </RULE>
    <RULE LABEL="Rule2">
      <CONDITION>true</CONDITION>
      <IMPLICATION>if ChairModel == "Executive" and Fabric == "Leather"
	then 	Armrests@default = true;
			Accessories.AdjustableArmrests@default = true;
end if;</IMPLICATION>
      <DESCRIPTION>Rule2</DESCRIPTION>
      <LHS>;armrests@default;accessories.adjustablearmrests@default;</LHS>
    </RULE>
  </RULES>
